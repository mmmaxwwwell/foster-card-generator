{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.foster.cardgenerator",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "debug.*",
    "window.*"
  ],
  "modes": {
    "window": {
      "title": "Foster Animals",
      "width": 1000,
      "height": 700,
      "minWidth": 600,
      "minHeight": 400,
      "center": true,
      "icon": "/resources/icons/appIcon.png"
    }
  },
  "cli": {
    "binaryName": "foster-app",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0"
  }
}